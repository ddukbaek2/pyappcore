{
    "3dsmax.defaultPath": "C:/Program Files/Autodesk/3ds Max 2023/3dsmaxbatch.exe",
    "blender.defaultPath": "C:/Program Files/Blender Foundation/Blender 4.0/blender.exe",
    "python.pythonPath": "C:/Program Files/Blender Foundation/Blender 4.0/4.0/Python/bin/python.exe",
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/Scripts/python.exe",
    "python.autoComplete.extraPaths": [
        "./hints",
        "./src"
    ],
    "python.analysis.extraPaths": [
        "./hints",
        "./src"
    ],
    "python.REPL.enableREPLSmartSend": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
	"[python]": {
		"editor.insertSpaces": false,
        "editor.tabSize": 4,
        "diffEditor.ignoreTrimWhitespace": false,
		"editor.formatOnType": true,
		"editor.wordBasedSuggestions": "off"
	},
    "terminal.integrated.showExitAlert": false,
    "launcher": {
        "launcherFilePath": "${workspaceFolder}/src/__launcher__.py",
        "debug": {
            "remotePort": 4885
        },
        "build": {
            "executeFileName": "New Project",
            "isHideConsole": false,
            "symbols": "DEBUG/LOG"
        },
        "arguments": [
            ""
        ]
    }
}